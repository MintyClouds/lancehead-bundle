HOSTNAME=domain.io

# Homer
HOMER_VERSION=21.07.1
HOMER_WEB_PORT=8080

# Wiki.js - PostgreSQL
WIKIJS_POSTGRES_VERSION=13.4-buster
WIKIJS_POSTGRES_PASSWORD=wikijspgpassword
WIKIJS_POSTGRES_USER=wikijs
WIKIJS_POSTGRES_DB=wikijs
WIKIJS_POSTGRES_DATA_PATH=/path/to/appstorage/wikijs/postgres

# Wiki.js
WIKIJS_VERSION=2.5
WIKIJS_WEB_PORT=3000
WIKIJS_POSTGRES_USER=wikijspguser
WIKIJS_POSTGRES_PASSWORD=wikijspgpassword
WIKIJS_POSTGRES_DB=wikijspgdb

# PiGallery 2
PIGALLERY_NGINX_VERSION=1.23.3
PIGALLERY_DATA_PATH=/path/to/appstorage/pigallery

PIGALLERY_MARIADB_VERSION=10.3
PIGALLERY_MARIADB_ROOT_PASSWORD=mysuperrootpassword
PIGALLERY_MARIADB_USER=pigallery
PIGALLERY_MARIADB_PASSWORD=mysuperpassword
PIGALLERY_MARIADB_DB=pigallery

PIGALLERY_VERSION=1.9.5
PIGALLERY_WEB_PORT=3000
PIGALLERY_GALLERY_PATH=/path/to/gallery/data
PIGALLERY_SCREENSHOTS_PATH=/path/to/screenshots/data
PIGALLERY_MEDIA_PATH=/path/to/media/data
PIGALLERY_UID=1000

PIGALLERY_NSFW_WEB_PORT=3000
PIGALLERY_NSFW_MARIADB_DB=pigallery-nsfw
PIGALLERY_NSFW_GALLERY_PATH=/path/to/gallery/data
PIGALLERY_NSFW_MEDIA_PATH=/path/to/media/data
